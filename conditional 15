import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        if((n%3==0)&&(n%7==0)){
            System.out.println("Number is divisible by 3 and 5");
        }
        else{
       System.out.println("Number is not divisible");
    }
}
}
