import java.util.Scanner;

public class ASCIIEvenOdd {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

       
        char ch = sc.next().charAt(0);

        int ascii = (int) ch;

        if (ascii % 2 == 0) {
            System.out.println("The ASCII value of '" + ch + "' is " + ascii + " and it is even.");
        } else {
            System.out.println("The ASCII value of '" + ch + "' is " + ascii + " and it is odd.");
        }
    }
}
